{
    "info": {
        "name": "Semantic Matching Engine API",
        "description": "Collection for testing the Candidate-Job Semantic Matching Engine.",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Candidates",
            "item": [
                {
                    "name": "Create Candidate",
                    "request": {
                        "method": "POST",
                        "header": [],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n    \"name\": \"Alice ICU\",\n    \"skill_description\": \"Registered nurse with 4 years ICU experience and German A2 level\",\n    \"experience\": 4,\n    \"location\": \"India\"\n}",
                            "options": {
                                "raw": {
                                    "language": "json"
                                }
                            }
                        },
                        "url": {
                            "raw": "{{base_url}}/candidates/",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "candidates",
                                ""
                            ]
                        }
                    },
                    "response": []
                },
                {
                    "name": "List All Candidates",
                    "request": {
                        "method": "GET",
                        "header": [],
                        "url": {
                            "raw": "{{base_url}}/candidates/",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "candidates",
                                ""
                            ]
                        }
                    },
                    "response": []
                }
            ]
        },
        {
            "name": "Jobs",
            "item": [
                {
                    "name": "Create Job",
                    "request": {
                        "method": "POST",
                        "header": [],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n    \"title\": \"Staff Nurse (ICU)\",\n    \"country\": \"Germany\",\n    \"description\": \"Searching for ICU nurses for a clinic in Berlin. German proficiency is a must.\"\n}",
                            "options": {
                                "raw": {
                                    "language": "json"
                                }
                            }
                        },
                        "url": {
                            "raw": "{{base_url}}/jobs/",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "jobs",
                                ""
                            ]
                        }
                    },
                    "response": []
                },
                {
                    "name": "List All Jobs",
                    "request": {
                        "method": "GET",
                        "header": [],
                        "url": {
                            "raw": "{{base_url}}/jobs/",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "jobs",
                                ""
                            ]
                        }
                    },
                    "response": []
                },
                {
                    "name": "Match Candidates for Job",
                    "request": {
                        "method": "GET",
                        "header": [],
                        "url": {
                            "raw": "{{base_url}}/jobs/1/match",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "jobs",
                                "1",
                                "match"
                            ]
                        },
                        "description": "Replace '1' with the actual Job ID you want to match."
                    },
                    "response": []
                }
            ]
        }
    ],
    "variable": [
        {
            "key": "base_url",
            "value": "http://127.0.0.1:8000",
            "type": "string"
        }
    ]
}